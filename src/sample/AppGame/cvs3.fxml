  <children>
    <AnchorPane fx:id="backdrop" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fx:id="viewport" fitHeight="400.0" fitWidth="640.0" onMouseMoved="#OnMouseMoved" onMousePressed="#OnMouseClicked" onMouseReleased="#OnMouseReleased" pickOnBounds="true" preserveRatio="true" />
         </children>
    </AnchorPane>
  </children>
</VBox>
